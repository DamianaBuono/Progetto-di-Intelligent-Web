                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.example.org/giappone#Attrazioni"/>
                    <rdf:Description rdf:about="http://www.example.org/giappone#Bar"/>
                    <rdf:Description rdf:about="http://www.example.org/giappone#Musei"/>
