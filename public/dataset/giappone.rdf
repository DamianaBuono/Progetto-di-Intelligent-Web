                    <rdf:Description rdf:about="http://www.example.org/giappone#Città"/>
                    <rdf:Description rdf:about="http://www.example.org/giappone#ParchiNazionali"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


